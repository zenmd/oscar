<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    author JZhang 
-->
<hibernate-mapping>
    <class name="com.quatro.model.Complaint" table="COMPLAINT">
        <id name="id" type="java.lang.Integer">
            <column name="ID" precision="10" scale="0" />
			<generator class="sequence">
   			 <param name="sequence">seq_complaint_id</param>
			</generator>
        </id>
        <property name="source" type="java.lang.String">
            <column name="SOURCE" length="100" />
        </property>
        <property name="method" type="java.lang.String">
            <column name="METHOD" length="20" />
        </property>
        <property name="firstname" type="java.lang.String">
            <column name="FIRSTNAME" length="30" />
        </property>
        <property name="lastname" type="java.lang.String">
            <column name="LASTNAME" length="30" />
        </property>
        <property name="standardsRelated" type="boolean">
            <column name="STANDARDSRELATED" />
        </property>
        <property name="standards" type="java.lang.String">
            <column name="STANDARDS" length="100" />
        </property>
        
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION"/>
        </property>
        <property name="satisfiedWithOutcome" type="java.lang.String">
            <column name="SATISFIED_WITH_OUTCOME" length="50" />
        </property>
        <property name="standardsBreached" type="java.lang.String">
            <column name="STANDARDS_BREACHED" length="1" />
        </property>
        <property name="outstandingIssues" type="java.lang.String">
            <column name="OUTSTANDING_ISSUES" length="4000" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS" length="1" />
        </property>
        <property name="completedDate" type="java.util.Calendar">
            <column name="COMPLETED_DATE" length="7" />
        </property>
        <property name="createdDate" type="java.util.Calendar">
            <column name="CREATED_DATE" length="7" />
        </property>
        <property name="duration" type="java.lang.String">
            <column name="DURATION" length="10" />
        </property>
        <property name="person1" type="java.lang.String">
            <column name="PERSON_1" length="60" />
        </property>
        <property name="title1" type="java.lang.String">
            <column name="TITLE_1" length="20" />
        </property>
        <property name="date1" type="java.util.Calendar">
            <column name="DATE_1" length="7" />
        </property>
        <property name="person2" type="java.lang.String">
            <column name="PERSON_2" length="60" />
        </property>
        <property name="title2" type="java.lang.String">
            <column name="TITLE_2" length="20" />
        </property>
        <property name="date2" type="java.util.Calendar">
            <column name="DATE_2" length="7" />
        </property>
        <property name="person3" type="java.lang.String">
            <column name="PERSON_3" length="60" />
        </property>
        <property name="title3" type="java.lang.String">
            <column name="TITLE_3" length="20" />
        </property>
        <property name="date3" type="java.util.Calendar">
            <column name="DATE_3" length="7" />
        </property>
        <property name="person4" type="java.lang.String">
            <column name="PERSON_4" length="60" />
        </property>
        <property name="title4" type="java.lang.String">
            <column name="TITLE_4" length="20" />
        </property>
        <property name="date4" type="java.util.Calendar">
            <column name="DATE_4" length="7" />
        </property>
        <property name="clientId" type="java.lang.Integer">
            <column name="CLIENT_ID" precision="10" scale="0" />
        </property>
        <property name="programId" type="java.lang.Integer">
            <column name="PROGRAM_ID" precision="10" scale="0" />
        </property>
        <property name="lastUpdateUser" column="lastupdateuser"></property>
        <property name="lastUpdateDate" column="lastupdatedate"></property>
    </class>
</hibernate-mapping>
